<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ResourceId>/subscriptions/5b20eb72-8725-4f00-8b04-871a7c712b90/resourceGroups/student-group/providers/Microsoft.Web/sites/hotel-backend</ResourceId>
    <ResourceGroup>student-group</ResourceGroup>
    <PublishProvider>AzureWebSite</PublishProvider>
